pragma solidity ^0.2.0;

contract MedStats {
	final address hospital;
	uint@_t2 count;
	mapping(address!_t3 => bool@_t4)@_t5 risk;

	@hospital
	 @@ {
		me == hospital
	}

	@hospital
	record @@ { { t2, t4, t6 }
		me == hospital
		t2 == t4
		t4 == t2/t3/me/all
		t6 == me/all
	}

	@c3
	check @@ { { t11 }
		me == c3
		t11 == me/all
	}
}
 